{
  "name": "gauss-jordan",
  "version": "1.1.8",
  "description": "Gauss Jordan Elimination",
  "main": "dist/gauss-jordan.js",
  "unpkg": "dist/gauss-jordan.min.js",
  "jsdelivr": "dist/gauss-jordan.js",
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "test": "jest --config jestconfig.json",
    "prepare": "yarn run build",
    "prepublishOnly": "yarn test && yarn run lint",
    "preversion": "yarn run lint",
    "version": "yarn run format && git add -A src",
    "postversion": "git push && git push --tags",
    "minify": "ncc build lib/index.js -o dist && rm -rf lib && mv dist lib",
    "build": "tsc && yarn run minify",
    "postbuild": "rollup -c"
  },
  "files": [
    "lib/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ian13456/gauss-jordan"
  },
  "keywords": [
    "math"
  ],
  "author": "Ian Huang",
  "license": "ISC",
  "homepage": "https://github.com/ian13456/gausee-jordan/#readme",
  "devDependencies": {
    "@types/jest": "^26.0.10",
    "@vercel/ncc": "^0.24.0",
    "jest": "^26.4.2",
    "prettier": "^2.1.1",
    "rollup": "^2.26.8",
    "rollup-plugin-node-resolve": "^5.2.0",
    "ts-jest": "^26.3.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.0.2"
  }
}
